  class Mover{
  PVector location;
  PVector velocity;
  PVector acceleration;
  int size;
  double mass,k, rx, ry;
  PApplet p;
  float xdelta, ydelta, r, g, b,op;
  PVector attract = new PVector(-1,-1);
  PVector mag = new PVector(0,0);
   Mover(PApplet p, double d, float x, float y, float r, float g, float b,float op){
    location = new PVector(x,y);
    velocity = new PVector(0,0);
    acceleration = new PVector(0,0);
    mass = d;
    this.p = p;
    size = (int)mass*2;
    this.r = r;
    this.g = g;
    this.b = b;
    this.op = op;
  }
  
  Mover(PApplet p, double d, float x, float y, float r, float g, float b, float op, PVector attract, PVector power){
    location = new PVector(x,y);
    velocity = new PVector(0,0);
    acceleration = new PVector(0,0);
    mass = d;
    this.p = p;
    size = (int)mass*7 + 3;
    this.r = r;
    this.g = g;
    this.b = b;
    this.op = op;
    this.attract = attract;
    mag = power;
  }
  
   void update(){
    velocity.add(acceleration);
    location.add(velocity);
    acceleration.mult(0);
//    checkEdges();
  }
   void checkEdges(){
    if(location.x + size >p.width){
      location.x = p.width-size;
      velocity.x *= -1;
    }else if (location.x -size < 0){
      location.x = size;
      velocity.x *= -1;
    }
    
    if(location.y + size/2 >= p.height) {
      velocity.y *= -1;
      location.y = p.height-size/2;
    }else if(location.y-size/2 < 0){
      velocity.y *= -1;
      location.y = size/2;
    }
    
  }
  boolean isInside(Liquid l){
    if(location.x > l.x && location.x < l.x + l.w && location.y > l.y && location.y < l.y + l.h){
      return true;
    }else
      return false;
  }
   PVector attract(Mover m){
    PVector force = PVector.sub(location,m.location);
    float distance = force.mag();
    distance = p.constrain(distance,5,25);
    force.normalize();
    float strength = (float) ((0.04*mass*m.mass)/(distance*distance));
    force.mult(strength);
    return force;
  }
  
   void drag(Liquid l){
    float speed = velocity.mag();
    float dragMagnitude = -1* l.c * speed * speed;
    
    PVector drag = velocity.get();
    drag.normalize();
    drag.mult(dragMagnitude);
    
    applyForce(drag);
  }
   void display(){
    p.noStroke();
    p.fill(r,g,b,op);
    p.ellipse(location.x, location.y, size, size);
  }
   void applyMouse(float magnemouse){
    ydelta = p.mouseY-location.y;
    xdelta = p.mouseX-location.x;
    k = 1*Math.sqrt(Math.pow(2, 2)/(Math.pow(xdelta, 2)+Math.pow(ydelta, 2)));
    rx = xdelta*k;
    ry = ydelta*k;
    PVector wi = new PVector((float)rx*magnemouse,(float)ry*magnemouse);
    wi.div((float)mass);
    acceleration.add(wi);
  }
  
  void perpetualForce(){
    ydelta = attract.y-location.y;
    xdelta = attract.x-location.x;
    k = 1*Math.sqrt(Math.pow(2, 2)/(Math.pow(xdelta, 2)+Math.pow(ydelta, 2)));
    rx = xdelta*k;
    ry = ydelta*k;
//    PVector wi = new PVector((float)rx,(float)ry);
    applyForce(new PVector((float)(rx*mag.x),(float)(ry*mag.y)));
  }
  
   void applyForce(PVector force){
    PVector f = force.get();
    f.div((float)mass);
    acceleration.add(f);
  }

}